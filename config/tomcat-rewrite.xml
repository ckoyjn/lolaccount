<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
		PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
		"http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
	<rule>
		<from>^/login.html</from>
		<to type="forward">/cms/admin/tologin.action</to>
	</rule>
	<rule>
		<from>^/logout.html</from>
		<to type="forward">/cms/admin/logout.action</to>
	</rule>
	<rule>
		<from>^/index.html</from>
		<to type="forward">/cms/admin/login.action</to>
	</rule>
</urlrewrite>
